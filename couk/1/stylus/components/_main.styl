html
	overflow-x hidden
body
	font-family Helvetica
	font-weight: normal
	font-size: 16px
	min-width: 320px
	position: relative
	line-height: 1.6
	-webkit-font-smoothing: antialiased
	overflow-x: hidden
	@media screen and (max-width: 991px)
		text-align center
		font-size 15px
.no-touch
	body
		font-family "ProximaNova" !important
section
	height 100vh
	@media screen and (max-width: 991px)
		height auto
	@media screen and (max-height 750px)
		height auto